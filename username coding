#include <stdio.h>
#include <string.h>

int main(void)
{
    char username[10];
    char password[10];

    printf("Enter your username: ");
    scanf("%s", username);

    printf("Enter your password: ");
    scanf("%s", password);

    if (strcmp(username, "doruk") == 0 && strcmp(password, "doruk28") == 0
        || strcmp(username, "anaxi") == 0 && strcmp(password, "2881") == 0
        || strcmp(username, "jules") == 0 && strcmp(password, "2234") == 0) {
            printf("Logged In Successfully");
    } else {
        printf("Incorrect username or password, please try again! ");
    }
}
